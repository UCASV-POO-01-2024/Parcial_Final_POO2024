    <children>
        <HBox prefHeight="0.0" prefWidth="335.0">
            <children>
                <Label prefHeight="24.0" prefWidth="68.0" text="Id del cliente" />
                <TextField fx:id="idCliente" prefHeight="25.0" prefWidth="164.0" />
            </children>
        </HBox>
        <Button fx:id="mostrarTarjetas" text="Mostrar Tarjetas" />
        <Label text="Tarjetas de Crédito:" />
        <Label fx:id="tarjetasCredito" />
        <Label text="Tarjetas de Débito:" />
        <Label fx:id="tarjetasDebito" />
    </children>
    <Button fx:id="volver" layoutX="10.0" layoutY="10.0" text="Volver" />

</VBox>

